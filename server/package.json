{
  "name": "trello-server",
  "version": "1.0.0",
  "description": "A Fullstack Application That Allows Custom Communication with Trello's REST API",
  "keywords": [
    "Trello Todo",
    "Trello ToDo App",
    "Trello To Do",
    "Trello To Do list",
    "Trello To Do App"
  ],
  "main": "js/index.js",
  "scripts": {
    "client-install": "cd ../client && npm install",
    "start": "tsc && node js/index.js",
    "server": "nodemon",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint . --ext .ts"
  },
  "author": "Boutrous Tawaifi",
  "dependencies": {
    "app": "^0.1.0",
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.5",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.8",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
