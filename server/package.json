{
  "name": "trello-server",
  "version": "1.0.0",
  "description": "A Fullstack Application That Allows Custom Communication with Trello's REST API",
  "keywords": [
    "Trello Todo",
    "Trello ToDo App",
    "Trello To Do",
    "Trello To Do list",
    "Trello To Do App"
  ],
  "main": "server.js",
  "scripts": {
    "client-install": "cd ../client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Boutrous Tawaifi",
  "dependencies": {
    "app": "^0.1.0",
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "nodemon": "^2.0.12",
    "redis": "^3.1.2"
  }
}
